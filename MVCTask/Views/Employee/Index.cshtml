@model List<Employee>
<h2>All Employees</h2>
<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Title</th>
            <th scope="col">Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var employee in Model)
        {
        <tr>
            <th scope="row">@employee.Id</th>
            <td>@employee.Name</td>
            <td>@employee.title</td>
            <td>@employee.Age</td>
        </tr>
        }
    </tbody>
</table>
